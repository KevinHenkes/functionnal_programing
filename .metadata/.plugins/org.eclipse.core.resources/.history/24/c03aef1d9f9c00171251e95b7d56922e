package fr.kevin.lambda;

import fr.kevin.lambda.entity.User;

public class Main {

	public static void main(String[] args) {
		User[] users;
		users.length = 3;
		for (int i = 0; i < 10; i++) {
			try {
				users.getClass().getDeclaredField("length").setAccessible(true);
			} catch (NoSuchFieldException | SecurityException e) {
				e.printStackTrace();
			}
		}
		
	}

}
